QLineEdit {
    font-family: "roboto light";
    font-size: 13px;
    border: 2px solid #adb4b9;
    border-radius: 12px;
    height: 24px;
}

QToolButton {
    height: 16px;
    width: 16px;
    padding: 4px;
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 2px;
    border-radius: 12px;
}

QToolButton:hover {
    background-color: #dee1e3;
}